<h1>New stock_quote</h1>

<% form_for(@stock_quote) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :stock %><br />
    <%= f.text_field :stock %>
  </p>
  <p>
    <%= f.label :quote_time %><br />
    <%= f.datetime_select :quote_time %>
  </p>
  <p>
    <%= f.label :open %><br />
    <%= f.text_field :open %>
  </p>
  <p>
    <%= f.label :close %><br />
    <%= f.text_field :close %>
  </p>
  <p>
    <%= f.label :high %><br />
    <%= f.text_field :high %>
  </p>
  <p>
    <%= f.label :low %><br />
    <%= f.text_field :low %>
  </p>
  <p>
    <%= f.label :volume %><br />
    <%= f.text_field :volume %>
  </p>
  <p>
    <%= f.label :adj_close %><br />
    <%= f.text_field :adj_close %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', stock_quotes_path %>