<h1>Listing data_sources</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
  </tr>

<% @data_sources.each do |data_source| %>
  <tr>
    <td><%=h data_source.name %></td>
    <td><%=h data_source.type %></td>
    <td><%= link_to 'Show', resource_path(data_source) %></td>
    <td><%= link_to 'Edit', edit_data_source_path(data_source) %></td>
    <td><%= link_to 'Destroy', resource_path(data_source), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New data_source', new_data_source_path %>